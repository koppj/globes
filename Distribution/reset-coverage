#! /bin/sh

echo "Remove all files generated by coverage testing..."

echo "source"
cd ..
cd source
rm -f *.bbg
rm -f *.bb
rm -f *.da
rm -f *.gcov

echo "examples"
cd ..
cd examples
rm -f *.bbg
rm -f *.bb
rm -f *.da
rm -f *.gcov

echo "modules"
cd ..
cd modules
rm -f *.bbg
rm -f *.bb
rm -f *.da
rm -f *.gcov

echo "libltdl"
cd ..
cd libltdl
rm -f *.bbg
rm -f *.bb
rm -f *.da
rm -f *.gcov