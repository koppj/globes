set terminal postscript eps enhanced 
set output 'systematic.eps'
set title "Output of systematic"

set key top left
set style data line
set xrange [-3:-1]
set yrange [0:25]
set xlabel "log sin^22{/Symbol q}_{13}"
set ylabel "{/Symbol D} {/Symbol c}^2"
plot 'sys-data0' title 'no systematic', 'sys-data1' title 'normalization + calibration', 'sys-data2' title '+ shape', 'sys-data3' title '+ bin-to-bin'